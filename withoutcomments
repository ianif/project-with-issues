function anotherInvalidFunction(param1, param2) {
    return param1 + param2;
    console.log("This line will never execute due to the return above"
}

const arrowFunction = () => {
    console.log("This is a valid arrow function but has issues"
    for (let i = 0; i < 5; i++) {
        console.log(i);
    // Missing closing brace for the for loop
}

class ExampleClass {
    constructor(name) {
        this.name = name;
    // Missing closing brace for the constructor
}

const exampleObject = {
    property1: "value1",
    property2: "value2"
    property3: "value3"
}

